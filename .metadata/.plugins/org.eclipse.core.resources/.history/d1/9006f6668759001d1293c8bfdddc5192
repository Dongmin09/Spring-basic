<<script type="text/javascript">
$(function(){
		$("#aSubmit").on("click",function(){
			$("#frm").submit();
		});
		// 결혼 유무
		$("#marriaged").on("click",function()){(
			let chkYn = $("#marriaged").is(":checked");
			
			if(chkYn){
				$("#marriaged").val("true");
			}else{
				$("#marriaged").val("false");
			}
		)};
});
</script>
